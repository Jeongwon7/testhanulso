<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hanulso.mapper.TimeMapper"><!-- mapper 태그로 시작한다, 주소는 인터페이스 주소 -->
  <select id="getTime" resultType="String"><!-- id는 추상 메서드 이름 -->
  	Select sysdate from dual <!-- 세미콜론 x  현재 시스템 날짜를 검색해서 듀얼 테이블에 출력해~-->
  </select>
</mapper>
